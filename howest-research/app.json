{
  "expo": {
    "name": "howest-research",
    "slug": "howest-research",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "howestresearch",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.jana-e.howest-research"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.janae.howestresearch"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/OpenSans/OpenSans-Bold.ttf",
            "./assets/fonts/OpenSans/OpenSans-BoldItalic.ttf",
            "./assets/fonts/OpenSans/OpenSans-ExtraBold.ttf",
            "./assets/fonts/OpenSans/OpenSans-ExtraBoldItalic.ttf",
            "./assets/fonts/OpenSans/OpenSans-Italic.ttf",
            "./assets/fonts/OpenSans/OpenSans-Light.ttf",
            "./assets/fonts/OpenSans/OpenSans-LightItalic.ttf",
            "./assets/fonts/OpenSans/OpenSans-Regular.ttf",
            "./assets/fonts/OpenSans/OpenSans-SemiBold.ttf",
            "./assets/fonts/OpenSans/OpenSans-SemiBoldItalic.ttf",

            "./assets/fonts/VAGRoundedStd/VAGRoundedStd-Black.otf",
            "./assets/fonts/VAGRoundedStd/VAGRoundedStd-Bold.otf",
            "./assets/fonts/VAGRoundedStd/VAGRoundedStd-Light.otf",
            "./assets/fonts/VAGRoundedStd/VAGRoundedStd-Thin.otf"
          ],
          "android": {
            "fonts": [
              { 
              "fontFamily": "VAGRounded",
              "fontDefinitions": [
                  {
                  "path": "./assets/fonts/VAGRoundedStd/VAGRoundedStd-Black.otf", weight: "900", style: "black"
                  },
                  {
                  "path": "./assets/fonts/VAGRoundedStd/VAGRoundedStd-Bold.otf", weight: "700", style: "bold"
                  },
                  {
                  "path": "./assets/fonts/VAGRoundedStd/VAGRoundedStd-Light.otf", weight: "300", style: "light"
                  },
                  {
                  "path": "./assets/fonts/VAGRoundedStd/VAGRoundedStd-Thin.otf", weight: "100", style: "thin"
                  }
                ]
              }
            ]
          },
          "ios": {
            "fonts": [
              "./assets/fonts/OpenSans/OpenSans-Bold.ttf",
              "./assets/fonts/OpenSans/OpenSans-BoldItalic.ttf",
              "./assets/fonts/OpenSans/OpenSans-ExtraBold.ttf",
              "./assets/fonts/OpenSans/OpenSans-ExtraBoldItalic.ttf",
              "./assets/fonts/OpenSans/OpenSans-Italic.ttf",
              "./assets/fonts/OpenSans/OpenSans-Light.ttf",
              "./assets/fonts/OpenSans/OpenSans-LightItalic.ttf",
              "./assets/fonts/OpenSans/OpenSans-Regular.ttf",
              "./assets/fonts/OpenSans/OpenSans-SemiBold.ttf",
              "./assets/fonts/OpenSans/OpenSans-SemiBoldItalic.ttf",

              "./assets/fonts/VAGRoundedStd/VAGRoundedStd-Black.otf",
              "./assets/fonts/VAGRoundedStd/VAGRoundedStd-Bold.otf",
              "./assets/fonts/VAGRoundedStd/VAGRoundedStd-Light.otf",
              "./assets/fonts/VAGRoundedStd/VAGRoundedStd-Thin.otf"
            ]
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}